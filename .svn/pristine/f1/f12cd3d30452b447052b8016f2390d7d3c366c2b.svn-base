			.alert{
				height: 120px;
				width: 200px;
				background: white;
				border-radius: 8px;
				border: 1px #dedede solid;
				position: absolute;
				top: 50%;
				left: 50%;
				transform: translate(-50%,-50%);
				z-index: 1200;
			}
			
			
			.change::after{
				content: "";
				width: 100%;
				height: 100%;
				background: black;
				opacity: 0.1;
				position: fixed;
				top: 0;left: 0;bottom: 0;right: 0;
				z-index: 1000;
			}
			.title{
				width: 170px;
				height: 30px;
				margin: 5px 0 0 0;
				line-height: 30px;
				text-align: center;
				font-weight: bold;
			}
			.content{
				width: 170px;
				height: 30px;
				line-height: 30px;
				text-align: center;
			}
			.o_line{
				height: 1px;
				width: 100%;
				background: #DEDEDE;
				transform: scaleY(0.5);
			}
			.footer{
				height: 30px;
				width: 170px;
				line-height: 30px;
				text-align: center;
			}
			button{
				border: none;
				cursor: pointer;
				outline: none;
				background: white;
			}
